{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "repoBaseUrl":{
            "type": "string",
            "metadata": {
                "description": "Base URL of the repository"
            }
        }},
  "functions": [],
  "variables": {},
  "resources": [
    {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "name": "global-policy",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('repoBaseUrl'), '/apim-devops-toolkit-demo-globalServicePolicy.template.json']"
        }
      }
    },{
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "name": "apis",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('repoBaseUrl'), '/apim-devops-toolkit-demo-apis.template.json']"
        }
      }
    },{
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "name": "calculator",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('repoBaseUrl'), '/basic-calculator/apim-devops-toolkit-demo-basic-calculator-api.template.json']"
        }
      }
    },{
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2021-04-01",
      "name": "globalPolicy-calculator",
      "properties": {
        "mode": "Incremental",
        "templateLink": {
          "uri": "[concat(parameters('repoBaseUrl'), '/basic-calculator/apim-devops-toolkit-demo-globalServicePolicy.template.json']"
        }
      }
    }
  ],
  "outputs": {}
}